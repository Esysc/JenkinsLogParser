{
    "manifest_version": 3,

    "name": "Log Parser for Jenkins",
    "version": "0.2.0",
    "description": "Enhanced console interface for Jenkins with log colorization and test navigation",
    "permissions": ["activeTab"],
    "content_scripts": [
        {
            "css": ["style.css"],
            "js": ["magic.js"],
            "run_at": "document_idle",
            "matches": ["http://*/*/*/*/console*", "https://*/*/*/*/console*"]
        }
    ]
}
