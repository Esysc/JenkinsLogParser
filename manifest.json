{
    "manifest_version": 3,

    "name": "Log Parser for Jenkins",
    "version": "0.3.0",
    "description": "Enhanced console interface for Jenkins with log colorization, filtering, search, and test navigation",
    "permissions": ["activeTab"],
    "content_scripts": [
        {
            "css": ["style.css"],
            "js": ["magic.js"],
            "run_at": "document_idle",
            "matches": [
                "http://*/*/console*",
                "https://*/*/console*",
                "http://*/*/consoleFull*",
                "https://*/*/consoleFull*",
                "http://*/*/flowGraphTable/*",
                "https://*/*/flowGraphTable/*",
                "http://localhost:*/test/*"
            ]
        }
    ]
}
