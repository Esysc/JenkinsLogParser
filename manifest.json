{
    "manifest_version": 3,

    "name": "Log Parser for Jenkins",
    "version": "0.3.2",
    "description": "Enhanced console interface for Jenkins with log colorization, filtering, search, and test navigation",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "content_scripts": [
        {
            "css": ["style.css"],
            "js": ["magic.js"],
            "run_at": "document_idle",
            "matches": [
                "http://*/*/console*",
                "https://*/*/console*",
                "http://*/*/consoleFull*",
                "https://*/*/consoleFull*",
                "http://*/*/flowGraphTable/*",
                "https://*/*/flowGraphTable/*",
                "http://*/job/*/log*",
                "https://*/job/*/log*"
            ]
        }
    ]
}
